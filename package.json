{
	"name": "horizon-toolkit-cli",
	"version": "0.9.52",
	"description": "horizon-engine cli",
	"main": "lib/app.js",
	"bin": {
		"hetool": "lib/app.js"
	},
	"scripts": {
		"start": "ts-node src/app.ts",
		"create": "npm run build && npm run test",
		"build": "tsc",
		"refresh": "rm -rf ./node_modules ./package-lock.json ./lib && npm install",
		"test": "mocha -r ts-node/register test/**/*.test.ts",
		"coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
		"dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
		"dev2": "nodemon --watch 'src/**' --exec \"ts-node\" src/index.ts",
		"watch": "nodemon src/index.ts",
		"debug": "tsc --sourcemap"
	},
	"keywords": [
		"horizon-engine",
		"gms",
		"gamemaker",
		"gms2"
	],
	"author": "devhorizon",
	"license": "MIT",
	"devDependencies": {
		"@types/chai": "^4.2.11",
		"@types/express": "^4.17.6",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.13.12",
		"@types/reflect-metadata": "^0.1.0",
		"@types/typescript": "^2.0.0",
		"chai": "^4.2.0",
		"fs": "0.0.1-security",
		"inversify": "^5.0.1",
		"mocha": "^8.0.1",
		"nodemon": "^2.0.4",
		"nyc": "^15.1.0",
		"reflect-metadata": "^0.1.13",
		"ts-node": "^8.10.2",
		"ts-node-dev": "^1.0.0-pre.44",
		"tslint": "^6.1.1",
		"typescript": "^3.9.5"
	},
	"dependencies": {
		"@types/js-base64": "^2.3.2",
		"chalk": "^4.1.0",
		"clear": "^0.1.0",
		"commander": "^5.1.0",
		"convert-excel-to-json": "^1.7.0",
		"figlet": "^1.4.0",
		"google-spreadsheet": "^3.0.11",
		"googleapis": "^39.2.0",
		"js-base64": "^2.6.2",
		"log4js": "^6.3.0",
		"path": "^0.12.7"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Alkapivo/horizon-engine-toolkit"
	}
}
